{
  "apache_ensure" : "present",
  "apache_use_dev" : "",
  "apache_dev_ensure" : "present",
  "apache_user" : "www-data",
  "apache_group" : "www-data",
  "apache_locale" : "",
  "apache_web_home" : "/var/www",
  "apache_access_file" : ".htaccess",
  "apache_timeout" : "300",
  "apache_keepalive" : "true"
  "apache_max_keepalive_requests" : "100",
  "apache_keepalive_timeout" : "5",
  "apache_mpm_prefork_start_servers" : "5",
  "apache_mpm_prefork_min_spare_servers" : "5",
  "apache_mpm_prefork_max_spare_servers" : "10",
  "apache_mpm_prefork_max_clients" : "150",
  "apache_mpm_prefork_max_requests_per_child" : "0",
  "apache_mpm_worker_start_servers" : "2",
  "apache_mpm_worker_min_spare_threads" : "25",
  "apache_mpm_worker_max_spare_threads" : "75",
  "apache_mpm_worker_thread_limit" : "64",
  "apache_mpm_worker_threads_per_child" : "25",
  "apache_mpm_worker_max_clients" : "150",
  "apache_mpm_worker_max_requests_per_child" : "0",
  "apache_mpm_event_start_servers" : "2",
  "apache_mpm_event_min_spare_threads" : "25",
  "apache_mpm_event_max_spare_threads" : "75",
  "apache_mpm_event_thread_limit" : "64",
  "apache_mpm_event_threads_per_child" : "25",
  "apache_mpm_event_max_clients" : "150",
  "apache_mpm_event_max_requests_per_child" : "0",
  "apache_ulimit_max_files" : "8192",
  "apache_restricted_files" : [ "^\.ht" ],
  "apache_default_type" : "None",
  "apache_hostname_lookups" : "",
  "apache_log_level" : "warn",
  "apache_log_formats" : {
    "%v:%p %h %l %u %t \"%r\" %>s %O \"%{Referer}i\" \"%{User-Agent}i\"" : "vhost_combined",
    "%h %l %u %t \"%r\" %>s %O \"%{Referer}i\" \"%{User-Agent}i\"" : "combined",
    "%h %l %u %t \"%r\" %>s %O" : "common",
    "%{Referer}i -> %U" : "referer",
    "%{User-agent}i" : "agent"
  },
  "apache_config_template" : "apache/apache2.conf.erb",
  "apache_vars_template" : "apache/envvars.erb",
  "apache_vhost_ip" : "*",
  "apache_default_port" : "80",
  "apache_http_port" : "80",
  "apache_use_ssl" : "",
  "apache_https_port" : "443",
  "apache_priority" : "25",
  "apache_options" : "Indexes FollowSymLinks MultiViews",  
  "apache_port_template" : "apache/ports.conf.erb",
  "apache_vhost_template" : "apache/vhost.conf.erb",
  "apache_vhost_proxy_template" : "apache/vhost-proxy.conf.erb",
  "apache_vhost_redirect_template" : "apache/vhost-redirect.conf.erb"
}
